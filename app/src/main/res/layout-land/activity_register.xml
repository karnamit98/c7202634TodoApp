<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/absbg5"
    tools:context=".activity_register">

  <TextView
      android:id="@+id/actLogin_labelNotLoggedIn2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:fontFamily="@font/montserrat"
      android:text="@string/not_registered_yet"
      android:textColor="#000000"
      android:textSize="14sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="@+id/actRegister_btnSignUp"
      app:layout_constraintEnd_toEndOf="@+id/imageView4"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/imageView4"
      app:layout_constraintTop_toBottomOf="@+id/imageView4"
      app:layout_constraintVertical_bias="0.42" />

  <Button
      android:id="@+id/actRegister_btnLogin"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:background="#00FFFFFF"
      android:fontFamily="@font/montserrat_subrayada"
      android:text="@string/log_in"
      android:textColor="#2962FF"
      android:textStyle="bold"
      app:backgroundTint="#00FFFFFF"
      app:layout_constraintBottom_toBottomOf="@+id/actLogin_labelNotLoggedIn2"
      app:layout_constraintEnd_toStartOf="@+id/activityRegister_txtConfirmPassword"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toEndOf="@+id/actLogin_labelNotLoggedIn2"
      app:layout_constraintTop_toTopOf="@+id/actLogin_labelNotLoggedIn2"
      app:layout_constraintVertical_bias="0.461" />

  <Button
      android:id="@+id/actLogin_btnRegister2"
      android:layout_width="199dp"
      android:layout_height="61dp"
      android:layout_marginStart="44dp"
      android:layout_marginTop="40dp"
      android:background="#00FFFFFF"
      android:fontFamily="@font/monoton"
      android:text="@string/sign_up"
      android:textColor="#FFFFFF"
      android:textSize="24sp"
      android:textStyle="bold"
      app:backgroundTint="#00FFFFFF"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <EditText
      android:id="@+id/activityRegister_txtPassword"
      android:layout_width="330dp"
      android:layout_height="30dp"
      android:layout_marginTop="12dp"
      android:autofillHints=""
      android:background="#E6FFFFFF"
      android:fontFamily="@font/montserrat"
      android:gravity="center"
      android:hint="@string/password"
      android:inputType="textPassword"
      android:paddingStart="5sp"
      android:paddingLeft="5sp"
      android:paddingTop="2sp"
      android:paddingEnd="5sp"
      android:paddingRight="5sp"
      android:paddingBottom="2sp"
      android:textColor="#000000"
      android:textSize="14sp"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="@+id/activityRegister_txtUsername"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/activityRegister_txtUsername"
      app:layout_constraintTop_toBottomOf="@+id/activityRegister_txtUsername" />

  <Button
      android:id="@+id/actRegister_btnSignUp"
      android:layout_width="185dp"
      android:layout_height="40dp"
      android:layout_marginTop="16dp"
      android:fontFamily="@font/aclonica"
      android:text="@string/sign_up"
      android:textSize="12sp"
      android:textStyle="bold"
      app:backgroundTint="#000000"
      app:cornerRadius="20dp"
      app:layout_constraintEnd_toEndOf="@+id/activityRegister_txtConfirmPassword"
      app:layout_constraintHorizontal_bias="0.475"
      app:layout_constraintStart_toStartOf="@+id/activityRegister_txtConfirmPassword"
      app:layout_constraintTop_toBottomOf="@+id/activityRegister_txtConfirmPassword" />

  <EditText
      android:id="@+id/activityRegister_txtUsername"
      android:layout_width="330dp"
      android:layout_height="30dp"
      android:layout_marginTop="12dp"
      android:autofillHints=""
      android:background="#E6FFFFFF"
      android:fontFamily="@font/montserrat"
      android:gravity="center"
      android:hint="@string/username"
      android:inputType="text"
      android:paddingStart="5sp"
      android:paddingLeft="5sp"
      android:paddingTop="2sp"
      android:paddingEnd="5sp"
      android:paddingRight="5sp"
      android:paddingBottom="2sp"
      android:textColor="#000000"
      android:textSize="14sp"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="@+id/activityRegister_txtFullName"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/activityRegister_txtFullName"
      app:layout_constraintTop_toBottomOf="@+id/activityRegister_txtFullName" />

  <EditText
      android:id="@+id/activityRegister_txtFullName"
      android:layout_width="330dp"
      android:layout_height="30dp"
      android:layout_marginTop="20dp"
      android:autofillHints=""
      android:background="#E6FFFFFF"
      android:fontFamily="@font/montserrat"
      android:gravity="center"
      android:hint="@string/full_name"
      android:inputType="textPersonName"
      android:paddingStart="5sp"
      android:paddingLeft="5sp"
      android:paddingTop="2sp"
      android:paddingEnd="5sp"
      android:paddingRight="5sp"
      android:paddingBottom="2sp"
      android:textColor="#000000"
      android:textSize="14sp"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.704"
      app:layout_constraintStart_toEndOf="@+id/actLogin_btnRegister2"
      app:layout_constraintTop_toTopOf="@+id/actLogin_btnRegister2" />

  <EditText
      android:id="@+id/activityRegister_txtConfirmPassword"
      android:layout_width="330dp"
      android:layout_height="30dp"
      android:layout_marginTop="12dp"
      android:autofillHints=""
      android:background="#E6FFFFFF"
      android:fontFamily="@font/montserrat"
      android:gravity="center"
      android:hint="@string/confirm_password"
      android:inputType="textPassword"
      android:paddingStart="5sp"
      android:paddingLeft="5sp"
      android:paddingTop="2sp"
      android:paddingEnd="5sp"
      android:paddingRight="5sp"
      android:paddingBottom="2sp"
      android:textColor="#000000"
      android:textSize="14sp"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="@+id/activityRegister_txtPassword"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/activityRegister_txtPassword"
      app:layout_constraintTop_toBottomOf="@+id/activityRegister_txtPassword" />

  <ImageView
      android:id="@+id/imageView4"
      android:layout_width="156dp"
      android:layout_height="85dp"
      android:layout_marginTop="28dp"
      android:contentDescription="@string/todo"
      app:layout_constraintBottom_toBottomOf="@+id/activityRegister_txtConfirmPassword"
      app:layout_constraintEnd_toEndOf="@+id/actLogin_btnRegister2"
      app:layout_constraintHorizontal_bias="0.72"
      app:layout_constraintStart_toStartOf="@+id/actLogin_btnRegister2"
      app:layout_constraintTop_toBottomOf="@+id/actLogin_btnRegister2"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/registerlogo" />

</androidx.constraintlayout.widget.ConstraintLayout>